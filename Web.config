<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="StudentRegistrationSystem" 
         connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=StudentRegistrationSystem;Integrated Security=True" 
         providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <compilation targetFramework="4.8" debug="true"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
	<system.webServer>
		<defaultDocument>
			<files>
				<clear />
				<add value="Index.aspx" />
			</files>
		</defaultDocument>
	</system.webServer>

</configuration>
